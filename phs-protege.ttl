:taxonomicReferenceSource rdf:type owl:DatatypeProperty ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( :Host
                                                      :SampleAnimal
                                                    )
                                      ] ;
                          rdfs:comment "" ;
                          skos:prefLabel "taxonomic reference source" .


###  https://linked.data.gov.au/def/phs#vernacularName
:vernacularName rdf:type owl:DatatypeProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Host
                                            :SampleAnimal
                                          )
                            ] ;
                rdfs:comment "The vernacular (common) name for the host or pest" ;
                skos:prefLabel "vernacular name" .


#################################################################
#    Classes
#################################################################

###  https://linked.data.gov.au/def/phs#Attractant
:Attractant rdf:type owl:Class ;
            rdfs:subClassOf :Bait ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dcterms:type ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :AttractantType
                            ] ;
            skos:definition "A substance that attracts Pests"@en ;
            skos:prefLabel "Attractant"@en .


###  https://linked.data.gov.au/def/phs#AttractantType
:AttractantType rdf:type owl:Class ;
                rdfs:subClassOf skos:Concept ;
                skos:definition "The subtype of a Attractant"@en ;
                skos:prefLabel "Attractant Type"@en .


###  https://linked.data.gov.au/def/phs#Bait
:Bait rdf:type owl:Class ;
      rdfs:subClassOf sosa:Actuator ,
                      sosa:Sampler .


###  https://linked.data.gov.au/def/phs#CaptureSystem
:CaptureSystem rdf:type owl:Class ;
               rdfs:subClassOf sosa:Sampler ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasBait ;
                                 owl:allValuesFrom :Bait
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTrap ;
                                 owl:allValuesFrom :Trap
                               ] ;
               rdfs:comment "A collection of things used to capture a Pest"@en ;
               skos:prefLabel "Capture System"@en .


###  https://linked.data.gov.au/def/phs#DiagnosticMethod
:DiagnosticMethod rdf:type owl:Class ;
                  rdfs:comment "A procedure or method used to determine the taxonomic identity of a plant pest. May be carried out in the field, or more usually, in a laboratory." ;
                  skos:prefLabel "Diagnostic method" .


###  https://linked.data.gov.au/def/phs#DiagnosticMethodType
:DiagnosticMethodType rdf:type owl:Class ;
                      rdfs:subClassOf skos:Concept ;
                      skos:definition "The subtype of a Diagnostic Method"@en ;
                      skos:prefLabel "Diagnostic Method Type"@en .


###  https://linked.data.gov.au/def/phs#Host
:Host rdf:type owl:Class ;
      rdfs:subClassOf :Stratum ,
                      dwc:Taxon ;
      owl:disjointWith :Substrate ;
           rdfs:comment "Particular pests are often associated with certain substrates or hosts. In the field, when targetting a pest, a surveyor will look for substrates or hosts that are more likely to harbour the target pest. Recording the substrateassists diagnosticians when they are identifying a specimen. It also aids analysis aimed at estimating pest prevalence based on absence records. Lastly, some protocols require the host or substrate to be recorded for each observation." ;
      skos:example "Bee; a Varroa Mite may be found in a bee's trachea" ;
      skos:prefLabel "Host" .


###  https://linked.data.gov.au/def/phs#HostType
:HostType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Host"@en ;
          skos:prefLabel "Host Type"@en .


###  https://linked.data.gov.au/def/phs#Lure
:Lure rdf:type owl:Class ;
      rdfs:subClassOf :Bait ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty dcterms:type ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :LureType
                      ] ;
      rdfs:comment "Lures are distinguished from Attractants in that Lures elicit behavioural responses in target organisms with high specificity. Pheremones and kairomones are examples of Lures. Foodstuffs or food mimics (such as protein or carbohydrate derivatives) produce responses in a wider range of organisms, and are thus classified as Attractants."@en ;
      rdfs:comment "A substance that lures Pests"@en ;
      skos:prefLabel "Lure"@en .


###  https://linked.data.gov.au/def/phs#LureType
:LureType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Lure"@en ;
          skos:prefLabel "Lure Type"@en .


###  https://linked.data.gov.au/def/phs#ObservationMethod
:ObservationMethod rdf:type owl:Class ;
                   rdfs:comment "A technique used to carry out an act of surveillance. Observation methods are chosen according to the target pest and/or host|substrate. The sensitivity of an observation may depend on the method that was used." ;
                   skos:prefLabel "Observation method" .


###  https://linked.data.gov.au/def/phs#ObservationMethodType
:ObservationMethodType rdf:type owl:Class ;
                       rdfs:subClassOf skos:Concept ;
                       skos:definition "The subtype of an Observation Method"@en ;
                       skos:prefLabel "Observation Method Type"@en .


###  https://linked.data.gov.au/def/phs#PestType
:PestType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Pest. Perhaps a single Taxon or perhaps a Group"@en ;
          skos:prefLabel "Pest Type"@en .


###  https://linked.data.gov.au/def/phs#Plant
:Plant rdf:type owl:Class ;
       rdfs:subClassOf dwc:Taxon .


###  https://linked.data.gov.au/def/phs#SampleAnimal
:SampleAnimal rdf:type owl:Class ;
              rdfs:subClassOf dwc:Taxon ,
                              sosa:Sample ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dcterms:type ;
                                owl:someValuesFrom :PestType
                              ] ;
              rdfs:comment "Any species, strain or biotype of plant, animal or pathogenic agent injurious to plants or plant products."@en ;
              rdfs:comment "xxx fix this: A Sample that is a Taxon and which may have a type that is a Pest Type"@en ;
              skos:prefLabel "Sample Animal"@en .


###  https://linked.data.gov.au/def/phs#Stratum
:Stratum rdf:type owl:Class ;
         rdfs:comment "Thing, material or organism on which a pest may occur. Stratum is abstract: instances should use either 'Host' or 'Substrate'." ;
           rdfs:comment "Particular pests are often associated with certain substrates or hosts. In the field, when targetting a pest, a surveyor will look for substrates or hosts that are more likely to harbour the target pest. Recording the substrate assists diagnosticians when they are identifying a specimen. It also aids analysis aimed at estimating pest prevalence based on absence records. Lastly, some protocols require the host or substrate to be recorded for each observation." ;
         skos:prefLabel "Stratum" .


###  https://linked.data.gov.au/def/phs#Substrate
:Substrate rdf:type owl:Class ;
           rdfs:subClassOf :Stratum ;
           rdfs:comment "Thing or material on or in which a pest may occur" ;
           rdfs:comment "A Substrate is a Stratum that is not a Host. Usually non-living, such as a rock or leaf litter." ;
           skos:prefLabel "Substrate" .


###  https://linked.data.gov.au/def/phs#SubstrateType
:SubstrateType rdf:type owl:Class ;
               rdfs:subClassOf skos:Concept ;
               skos:definition "The subtype of a Substrate"@en ;
               skos:prefLabel "Substrate Type"@en .


###  https://linked.data.gov.au/def/phs#TaxonOccurrence
:TaxonOccurrence rdf:type owl:Class ;
                   rdfs:subClassOf sosa:FeatureOfInterest ;
                   skos:definition "The aggregated observations of a Taxon leading to knowledge of its distribution"@en ;
                   skos:editorialNote "This FoI is an intermediate FoI en route to a pest distribution which will inform pest management plans. This FoI is not indicated as being a spatio-temporal thing since it may require significant enhancement and modelling using this taxon occurrence information to generate a reliable spatio-temporal taxon distribution" ;
                   skos:prefLabel "Taxon occurrence"@en .


###  https://linked.data.gov.au/def/phs#TaxonIdentification
:TaxonIdentification rdf:type owl:Class ;
                     rdfs:subClassOf sosa:Observation ;
                     skos:definition "The identification of the Taxon of an organism"@en ;
                     skos:prefLabel "Taxon Identification"@en .


###  https://linked.data.gov.au/def/phs#Trap
:Trap rdf:type owl:Class ;
      rdfs:subClassOf sosa:Sampler ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty dcterms:type ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :TrapType
                      ] ;
      rdfs:comment "A device that catches or collects organisms or evidence of them." ;
      skos:prefLabel "Trap" .


###  https://linked.data.gov.au/def/phs#TrapType
:TrapType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Trap"@en ;
          skos:prefLabel "Trap Type" .


###  https://linked.data.gov.au/def/phs#Capturing
:Capturing rdf:type owl:Class ;
          rdfs:subClassOf sosa:Observation ,
                          sosa:Sampling ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:hasResult ;
                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                            owl:onClass sosa:Sample
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:madeBySampler ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :CaptureSystem
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:usedProcedure ;
                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                            owl:onClass sosa:Procedure
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <sosa:resultTime/sosa:phenomenonTime> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:hasFeatureOfInterest ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :TaxonDistribution
                          ] ;
          skos:definition "The capturing of an Animal"@en ;
          skos:prefLabel "Capturing"@en .


