###  https://linked.data.gov.au/def/phs#catchesPest
:catchesPest rdf:type owl:ObjectProperty ;
             rdfs:domain :Trap ;
             rdfs:range :SampleAnimal ;
             rdfs:comment "A device that is designed to catch the target pest"@en ,
                          "An object which provides refuge to the target pest, and can thus be used as a trap"@en ;
             skos:prefLabel "catches pest"@en .


###  https://linked.data.gov.au/def/phs#detectsPest
:detectsPest rdf:type owl:ObjectProperty ;
             rdfs:domain :ObservationMethod ;
             rdfs:range :SampleAnimal ;
             rdfs:comment "The pest for which the observation method is useful"@en ;
             skos:prefLabel "detects pest"@en .


###  https://linked.data.gov.au/def/phs#hasBait
:hasBait rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:hasPart ;
         rdfs:domain :CaptureSystem ;
         rdfs:range :Bait ;
         skos:definition "A Capture System may have a Bait as one of its parts"@en ;
         skos:prefLabel "has bait"@en .


###  https://linked.data.gov.au/def/phs#hasTrap
:hasTrap rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:hasPart ;
         rdfs:domain :CaptureSystem ;
         rdfs:range :Trap ;
         skos:definition "A Capture System must have a Trap as one of its parts"@en ;
         skos:prefLabel "has trap"@en .


###  https://linked.data.gov.au/def/phs#hostsPest
:hostsPest rdf:type owl:ObjectProperty ;
           rdfs:domain :Host ;
           rdfs:range :SampleAnimal ;
           rdfs:comment "A living thing (usually a plant) on which the target pest may be found"@en ;
           skos:prefLabel "hosts pest"@en .


###  https://linked.data.gov.au/def/phs#identifiesPest
:identifiesPest rdf:type owl:ObjectProperty ;
                rdfs:domain :DiagnosticMethod ;
                rdfs:range :SampleAnimal ;
                rdfs:comment "This diagnostic method is appropriate for the target pest"@en ;
                skos:prefLabel "Identifies pest"@en .


###  https://linked.data.gov.au/def/phs#lureType
:lureType rdf:type owl:ObjectProperty ;
          rdfs:domain :Lure ;
          rdfs:range skos:Concept .


###  https://linked.data.gov.au/def/phs#luresPest
:luresPest rdf:type owl:ObjectProperty ;
           rdfs:domain :Lure ;
           rdfs:range :SampleAnimal ;
           rdfs:comment "The target pest which is attracted by this lure"@en ;
           skos:prefLabel "lures pest"@en .


###  https://linked.data.gov.au/def/phs#supportsPest
:supportsPest rdf:type owl:ObjectProperty ;
              rdfs:domain :Substrate ;
              rdfs:range :SampleAnimal ;
              rdfs:comment "A substrate on which the target pest is likely be found, if it were present"@en ;
              skos:prefLabel "supports pest"@en .


###  https://linked.data.gov.au/def/phs#trapType
:trapType rdf:type owl:ObjectProperty ;
          rdfs:domain :Trap ;
          rdfs:range :TrapType ;
          rdfs:comment "The make or model of a trap" ;
          skos:prefLabel "trap type" .


###  http://purl.org/dc/terms/hasPart
dcterms:hasPart rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/type
dcterms:type rdf:type owl:ObjectProperty ;
             skos:prefLabel "type"@en .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdf:type owl:ObjectProperty .


###  sosa:resultTime/sosa:phenomenonTime
<sosa:resultTime/sosa:phenomenonTime> rdf:type owl:ObjectProperty ;
    rdfs:comment "https://zhiduozhang.github.io/sdw/ssn/#phenomenonTime" i;
    skos:definition "The time that the Result of an Observation/Actuation applies to the FeatureOfInterest. Not necessarily the same as the result-time. May be an interval or an instant, or some other compound temporal entity." .


