<http://linked.data.gov.au/def/phs> rdf:type owl:Ontology ;
                                     owl:versionIRI <http://linked.data.gov.au/def/phs/0.1> ;
                                     owl:imports sosa: ;
                                     dcterms:contributor <https://orcid.org/0000-0002-3884-3420> ;
                                     dcterms:created "2020-02-11"^^xsd:date ;
                                     dcterms:creator <https://orcid.org/0000-0003-3357-2307> ,
                                                     <https://orcid.org/0000-0002-8742-7730> ;
                                     dcterms:modified "2020-06-17"^^xsd:date ;
                                     dcterms:publisher <https://linked.data.gov.au/org/dawe> ;
                                     skos:definition "An ontology for surveillance events relating to plant pests. These events comprise field observations and associated diagnostic work (i.e. taxonomic identification) of samples taken as part of the observation. Surveillance is one of the core activities of national plant protection organizations (NPPOs). It provides NPPOs with a technical basis for many phytosanitary measures; including phytosanitary import requirements, pest free areas, pest reporting and eradication, and pest status in an area. All terms are defined by ISPM 5."@en ;
                                     skos:prefLabel "Plant Health Surveillance Ontology"@en ;
                                     sdo:codeRepository "https://github.com/AGLDWG/phs-ont"^^xsd:anyURI .


#################################################################
#    Object Properties
#################################################################

###  http://linked.data.gov.au/def/phs#catchesPest
:catchesPest rdf:type owl:ObjectProperty ;
             rdfs:domain :Trap ;
             rdfs:range :SampleAnimal ;
             rdfs:comment "A device that is designed to catch the target pest"@en ,
                          "An object which provides refuge to the target pest, and can thus be used as a trap"@en ;
             skos:prefLabel "catches pest"@en .


###  http://linked.data.gov.au/def/phs#detectsPest
:detectsPest rdf:type owl:ObjectProperty ;
             rdfs:domain :ObservationMethod ;
             rdfs:range :SampleAnimal ;
             rdfs:comment "The pest for which the observation method is useful"@en ;
             skos:prefLabel "detects pest"@en .


###  http://linked.data.gov.au/def/phs#hasBait
:hasBait rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:hasPart ;
         rdfs:domain :CaptureSystem ;
         rdfs:range :Bait ;
         skos:definition "A Capture System may have a Bait as one of its parts"@en ;
         skos:prefLabel "has bait"@en .


###  http://linked.data.gov.au/def/phs#hasTrap
:hasTrap rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf dcterms:hasPart ;
         rdfs:domain :CaptureSystem ;
         rdfs:range :Trap ;
         skos:definition "A Capture System must have a Trap as one of its parts"@en ;
         skos:prefLabel "has trap"@en .


###  http://linked.data.gov.au/def/phs#hostsPest
:hostsPest rdf:type owl:ObjectProperty ;
           rdfs:domain :Host ;
           rdfs:range :SampleAnimal ;
           rdfs:comment "A living thing (usually a plant) on which the target pest may be found"@en ;
           skos:prefLabel "hosts pest"@en .


###  http://linked.data.gov.au/def/phs#identifiesPest
:identifiesPest rdf:type owl:ObjectProperty ;
                rdfs:domain :DiagnosticMethod ;
                rdfs:range :SampleAnimal ;
                rdfs:comment "A diagnostic method that is appropriate for the target pest"@en ;
                skos:prefLabel "Identifies pest"@en .


###  http://linked.data.gov.au/def/phs#lureType
:lureType rdf:type owl:ObjectProperty ;
          rdfs:domain :Lure ;
          rdfs:range skos:Concept .

###  http://linked.data.gov.au/def/phs#luresPest
:luresPest rdf:type owl:ObjectProperty ;
           rdfs:domain :Lure ;
           rdfs:range :SampleAnimal ;
           rdfs:comment "A target pest that is attracted by this lure"@en ;
           skos:prefLabel "lures pest"@en .

###  http://linked.data.gov.au/def/phs#supportsPest
:supportsPest rdf:type owl:ObjectProperty ;
              rdfs:domain :Substrate ;
              rdfs:range :SampleAnimal ;
              rdfs:comment "A substrate on which the target pest is likely be found, if it were present in the area"@en ;
              skos:prefLabel "supports pest"@en .

###  http://linked.data.gov.au/def/phs#trapType
:trapType rdf:type owl:ObjectProperty ;
          rdfs:domain :Trap ;
          rdfs:range :TrapType ;
          rdfs:comment "The make or model of a trap" ;
          skos:prefLabel "trap type" .


#################################################################
#    Data properties
#################################################################

###  http://linked.data.gov.au/def/phs#taxonomicReferenceSource
:taxonomicReferenceSource rdf:type owl:DatatypeProperty ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( :Host
                                                      :SampleAnimal
                                                    )
                                      ] ;
                          rdfs:comment "" ;
                          skos:prefLabel "taxonomic reference source" .

###  http://linked.data.gov.au/def/phs#vernacularName
:vernacularName rdf:type owl:DatatypeProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :Host
                                            :SampleAnimal
                                          )
                            ] ;
                rdfs:comment "The vernacular (common) name for the host or pest" ;
                skos:prefLabel "vernacular name" .


#################################################################
#    Classes
#################################################################

###  http://linked.data.gov.au/def/phs#Attractant
:Attractant rdf:type owl:Class ;
            rdfs:subClassOf :Bait ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dcterms:type ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :AttractantType
                            ] ;
            skos:definition "A substance that attracts Pests"@en ;
            skos:prefLabel "Attractant"@en .


###  http://linked.data.gov.au/def/phs#AttractantType
:AttractantType rdf:type owl:Class ;
                rdfs:subClassOf skos:Concept ;
                skos:definition "The subtype of a Attractant"@en ;
                skos:prefLabel "Attractant Type"@en .


###  http://linked.data.gov.au/def/phs#Bait
:Bait rdf:type owl:Class ;
      rdfs:subClassOf sosa:Actuator ,
                      sosa:Sampler .


###  http://linked.data.gov.au/def/phs#CaptureSystem
:CaptureSystem rdf:type owl:Class ;
               rdfs:subClassOf sosa:Sampler ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasBait ;
                                 owl:allValuesFrom :Bait
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasTrap ;
                                 owl:allValuesFrom :Trap
                               ] ;
               rdfs:comment "A collection of things used to capture or collect a Pest"@en ;
               skos:prefLabel "Capture System"@en .


###  http://linked.data.gov.au/def/phs#DiagnosticMethod
:DiagnosticMethod rdf:type owl:Class ;
                  rdfs:comment "A procedure or method used to determine the taxonomic identity of a plant pest. May be carried out in the field, or more usually, in a laboratory." ;
                  skos:prefLabel "Diagnostic method" .


###  http://linked.data.gov.au/def/phs#DiagnosticMethodType
:DiagnosticMethodType rdf:type owl:Class ;
                      rdfs:subClassOf skos:Concept ;
                      skos:definition "The subtype of a Diagnostic Method"@en ;
                      skos:prefLabel "Diagnostic Method Type"@en .


###  http://linked.data.gov.au/def/phs#Host
:Host rdf:type owl:Class ;
      rdfs:subClassOf :Stratum ,
                      dwc:Taxon ;
      owl:disjointWith :Substrate ;
      rdfs:comment "A Plant or Animal (a Taxon) that is also a Stratum." ;
      skos:example "Bee; a Varroa Mite may be found in a bee's trachea" ;
      skos:prefLabel "Host" .


###  http://linked.data.gov.au/def/phs#HostType
:HostType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Host"@en ;
          skos:prefLabel "Host Type"@en .


###  http://linked.data.gov.au/def/phs#Lure
:Lure rdf:type owl:Class ;
      rdfs:subClassOf :Bait ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty dcterms:type ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :LureType
                      ] ;
      rdfs:comment "A substance that lures Pests"@en ;
      rdfs:comment "Lures are distinguished from Attractants in that Lures elicit behavioural responses in target organisms with high specificity. Pheremones and kairomones are examples of Lures. Foodstuffs or food mimics (such as protein or carbohydrate derivatives) produce responses in a wider range of organisms, and are thus classified as Attractants."@en ;
      skos:prefLabel "Lure"@en .


###  http://linked.data.gov.au/def/phs#LureType
:LureType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Lure"@en ;
          skos:prefLabel "Lure Type"@en .


###  http://linked.data.gov.au/def/phs#ObservationMethod
:ObservationMethod rdf:type owl:Class ;
                   rdfs:comment "A technique used to carry out an act of surveillance. Observation methods are chosen according to the target pest and/or host|substrate. The sensitivity of an observation may depend on the method that was used." ;
                   skos:prefLabel "Observation method" .


###  http://linked.data.gov.au/def/phs#ObservationMethodType
:ObservationMethodType rdf:type owl:Class ;
                       rdfs:subClassOf skos:Concept ;
                       skos:definition "The subtype of an Observation Method"@en ;
                       skos:prefLabel "Observation Method Type"@en .


###  http://linked.data.gov.au/def/phs#PestType
:PestType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Pest. Perhaps a single Taxon or perhaps a Group"@en ;
          skos:prefLabel "Pest Type"@en .


###  http://linked.data.gov.au/def/phs#Plant
:Plant rdf:type owl:Class ;
       rdfs:subClassOf dwc:Taxon .


###  http://linked.data.gov.au/def/phs#SampleAnimal
:SampleAnimal rdf:type owl:Class ;
              rdfs:subClassOf dwc:Taxon ,
                              sosa:Sample ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dcterms:type ;
                                owl:someValuesFrom :PestType
                              ] ;
              rdfs:comment "A Sample that is a Taxon and which may have a type that is a Pest Type"@en ;
              skos:prefLabel "Sample Animal"@en .


###  http://linked.data.gov.au/def/phs#Stratum
:Stratum rdf:type owl:Class ;
         rdfs:comment "Thing or material on which a pest is searched for or found. Stratum is abstract: instances should use either 'Host' or 'Substrate'." ;
         skos:prefLabel "Stratum" .


###  http://linked.data.gov.au/def/phs#Substrate
:Substrate rdf:type owl:Class ;
           rdfs:subClassOf :Stratum ;
           rdfs:comment "A Stratum that is not a Host. Usually non-living, such as a rock." ;
           skos:prefLabel "Substrate" .


###  http://linked.data.gov.au/def/phs#SubstrateType
:SubstrateType rdf:type owl:Class ;
               rdfs:subClassOf skos:Concept ;
               skos:definition "The subtype of a Substrate"@en ;
               skos:prefLabel "Substrate Type"@en .


###  http://linked.data.gov.au/def/phs#TaxonOccurence
:TaxonOccurence rdf:type owl:Class ;
                   rdfs:subClassOf sosa:FeatureOfInterest ;
                   skos:definition "The aggregated observations of a Taxon leading to knowledge of its distribution"@en ;
                   skos:editorialNote "This FoI is an intermediate FoI en route to a pest distribution which will inform pest management plans. This FoI is not indicated as being a spatio-temporal thing since it may require significant enhancement and modelling using this taxon occurence information to generate a reliable spatio-temporal taxon distribution" ;
                   skos:prefLabel "Taxon Occurence"@en .


###  http://linked.data.gov.au/def/phs#TaxonIdentification
:TaxonIdentification rdf:type owl:Class ;
                     rdfs:subClassOf sosa:Observation ;
                     skos:definition "The identification of the Taxon of an organism"@en ;
                     skos:prefLabel "Taxon Identification"@en .


###  http://linked.data.gov.au/def/phs#Trap
:Trap rdf:type owl:Class ;
      rdfs:subClassOf sosa:Sampler ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty dcterms:type ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :TrapType
                      ] ;
      rdfs:comment "A device that catches or collects organisms or evidence of them." ;
      skos:prefLabel "Trap" .


###  http://linked.data.gov.au/def/phs#TrapType
:TrapType rdf:type owl:Class ;
          rdfs:subClassOf skos:Concept ;
          skos:definition "The subtype of a Trap"@en ;
          skos:prefLabel "Trap Type" .


###  http://linked.data.gov.au/def/phs#Capturing
:Capturing rdf:type owl:Class ;
          rdfs:subClassOf sosa:Observation ,
                          sosa:Sampling ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:hasResult ;
                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                            owl:onClass sosa:Sample
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:madeBySampler ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :CaptureSystem
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:usedProcedure ;
                            owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                            owl:onClass sosa:Procedure
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2006/time#hasTime> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sosa:hasFeatureOfInterest ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :TaxonDistribution
                          ] ;
          skos:definition "The capturing of an Animal"@en ;
          skos:prefLabel "Capturing"@en .

