@prefix phs: <https://github.com/F3RR3T/PlantHealthSurveillanceOntology/blob/master/phs.ttl .

@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/terms/> .           # DCMI gently encourages use of the /terms/ namespace (over /elements/1.1/)
@prefix dwc: <http://rs.tdwg.org/dwc/terms/> .
@prefix tgn: <http://vocab.getty.edu/tgn/> .         # Getty Thesaurus of Geographic Names
# See https://prefix.cc/ for rdf namespace lookups
 
#Classes
phs:InsectTrap
  a owl:Class ;
  rdfs:label "A device that catches or collects insects" ;
  rdfs:seeAlso <http://dbpedia.org/resource/Insect_trap> ;
.
phs:Lure
  a owl:Class ;
  rdfs:label "An attractant that may be placed in an InsectTrap" ;
.
phs:Stratum
  a rdfs:Thing ;
  rdfs:label "Thing or material on which a pest is searched for or found upon. A superclass for host or substrate." ;
.
phs:Substrate 
  a owl:Class ;
  rdfs:subClassOf phs:Stratum ;
  rdfs:label "A non-living thing on which a pest may be found." ;
.
rdfs:Host
  rdfs:subClassOf phs:Stratum ;
  rdfs:subClassOf dwc:Taxon ;
  rdfs:label "A plant on (or in) which a pest may be found." ;
.
#Properties
phs:trapType
  a owl:DatatypeProperty ;
  rdfs:domain phs:InsectTrap ;
  rdfs:comment "should we declarea range here for allowed types?" ;
  rdfs:label "The make or model of a trap" ;
.
# Might be better to create the term 'TrapModel' for commercial names of trap.
# That would leave 'TrapType' for ['Light trap', 'Pitfall trap', 'Adhesive trap',
#   'Flying insect trap', ...]
# Or do we use the RANGE to define allowed values?
phs:TrapTypes
  a skos:Collection ;
  rdfs:label "Collection of allowed names for trap types" ;
  skos:member ffly:Lyndfield ;
  skos:member ffly:Peyton;
  skos:member ffly:AnotherOne;
.
# I have introduced the ffly namespace specifically for fruit fly terms
ffly:Lyndfield
  a phs:trapType ;
.
ffly:Peyton
  a phs:trapType ;
.
ffly:AnotherOne
  a phs:trapType ;
.
phs:lureType
  a owl:DatatypeProperty ;
  rdfs:domain phs:Lure ;
.
# Make a Canoncial collection of lure type
# TODO - Alias list to collect wild-type variations of accepted terms
# TaxaaS (Taxonomy as a Service) has been depricated by the dept, so I'll use it 
# to refer to the API of any taxonomic reference system (GBIF, ALA, 'TRS').


#TARGET PEST
phs:TargetPest 
  a dwc:Taxon ;
  owl:equivalentClass dwc:Taxon ;
  rdfs:label "The pest taxon that is is the subject of surveillance. Can have more than one target." ;
.
phs:targetTaxonomicReferenceSource
  a dc:Dataset ;
  rdfs:label "The name of the taxonomic reference system used for this target" ;
  rdfs:comment "Placeholder Class relationship" ;
.
phs:targetTaxonID
  a phs:TargetPest ;
  rdfs:label "Identifier for taxon proprties (including the following) of the Target Pest" ;
  owl:equivalentProperty dwc:taxonID ;
  rdfs:comment "Automatically populated by TaxaaS. Unsure whether we need taxonID AND taxonConceptID" ;
.
phs:targetTaxonConceptID
  a phs:TargetPest ;
  rdfs:label "Identifier for taxonomic CONCEPT (not nomenclature) of the Target Pest" ;
  owl:equivalentProperty dwc:taxonConceptID ;
  rdfs:comment "Automatically populated by TaxaaS. Unsure whether we need taxonID AND taxonConceptID" ;
.
phs:targetCommonName
  a phs:TargetPest ;
  rdfs:label "The vernacular (common) name for the Target Pest" ;
  owl:equivalentProperty dwc:vernacularName ;
  rdfs:comment "I'd prefer to use targetVernacularName and then do a 'common sameas vernacular'" ;
.
phs:targetScientificName
  a phs:TargetPest ;
  owl:equivalentProperty dwc:scientificName ;
  rdfs:label "The scientific name of the target pest" ;
  rdfs:comment "I changed this from taxonName to align with DwC'" ;
.
# Other properties such as rank, parent taxon can be inferred from TaxaaS.

# HOST
phs:Host
  a dwc:Taxon ;
  owl:equivalentClass dwc:Taxon ;
  rdfs:label "The host taxon that is is the subject of surveillance. Can have more than one host." ;
.
phs:hostTaxonomicReferenceSource
  a dc:Dataset ;
  rdfs:label "The name of the taxonomic reference system used for this host" ;
  rdfs:comment "Placeholder Class relationship" ;
.
phs:hostTaxonID
  a phs:Host ;
  rdfs:label "Identifier for taxon properties (including the following) of the host" ;
  owl:equivalentProperty dwc:taxonID ;
  rdfs:comment "Automatically populated by TaxaaS. Unsure whether we need taxonID AND taxonConceptID" ;
.
phs:hostTaxonConceptID
  a phs:Host ;
  rdfs:label "Identifier for taxonomic CONCEPT (not nomenclature) of the host" ;
  owl:equivalentProperty dwc:taxonConceptID ;
  rdfs:comment "Automatically populated by TaxaaS. Unsure whether we need taxonID AND taxonConceptID" ;

phs:hostCommonName
  a phs:Host ;
  rdfs:label "The vernacular (common) name for the host" ;
  owl:equivalentProperty dwc:vernacularName ;
  rdfs:comment "I'd prefer to use hostVernacularName and then do a 'common sameas vernacular'" ;
.
phs:hostScientificName
  a phs:Host ;
  owl:equivalentProperty dwc:scientificName ;
  rdfs:label "The scientific name of the host" ;
  rdfs:comment "I changed this from taxonName to align with DwC'" ;
.
